@using WebApplication1.Models
@{
}
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
        <table width="100%" class="table table-striped table-bordered">
            <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Url</td>
                <td>Genre</td>
                <td>Year</td>
                <td>Director</td>
                <td>User</td>
            </tr>
            </thead>
            <tbody>
            @foreach (Movie item in ViewBag.Movies)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Url</td>
                    <td>@item.Genre.ToString()</td>
                    <td>@item.Year</td>
                    <td>@item.Director</td>
                    <td>@(item.User?.UserName)</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>

